  "config": {
    "blanket": {
      "pattern": "lib",
      "data-cover-never": "node_modules",
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "devDependencies": {
    "blanket": "~1.1.6",
    "chai": "~1.9.1",
    "coveralls": "^2.10.0",
    "istanbul": "^0.2.10",
    "mocha": "~1.19.0"
  }
}
